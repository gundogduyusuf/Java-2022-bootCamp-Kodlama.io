package mükemmelSayı;

public class Main {

	public static void main(String[] args) {
		// Kendinden başka pozitif tüm tam bölenlerin toplamı kendisine eşit olan sayıya
		// mükemmel sayı denir
		// 6--->1,2,3
		// 28-->1,2,4,7,14
		int number = 5;
		int total = 0;
		for (int i = 1; i < number; i++) {
			if (number % i == 0) {
				total = total + i;
			}

		}

	}

}
